{
  "functions": {
    "agents/weather/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "agents/finance/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "agents/code/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "agents/master/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/weather/(.*)",
      "dest": "/agents/weather/handler.py"
    },
    {
      "src": "/finance/(.*)",
      "dest": "/agents/finance/handler.py"
    },
    {
      "src": "/code/(.*)",
      "dest": "/agents/code/handler.py"
    },
    {
      "src": "/master/(.*)",
      "dest": "/agents/master/handler.py"
    },
    {
      "src": "/",
      "dest": "/agents/master/handler.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@OPENAI_API_KEY",
    "CONTEXT7_API_KEY": "@CONTEXT7_API_KEY", 
    "LEETCODE_SESSION": "@LEETCODE_SESSION"
  },
  "build": {
    "env": {
      "PYTHONPATH": "./shared"
    }
  },
  "buildCommand": "",
  "installCommand": "",
  "framework": null
}