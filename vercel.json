{
  "functions": {
    "agents/weather/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "agents/finance/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "agents/code/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "agents/master/handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/weather/(.*)",
      "dest": "/agents/weather/handler.py"
    },
    {
      "src": "/finance/(.*)",
      "dest": "/agents/finance/handler.py"
    },
    {
      "src": "/code/(.*)",
      "dest": "/agents/code/handler.py"
    },
    {
      "src": "/master/(.*)",
      "dest": "/agents/master/handler.py"
    },
    {
      "src": "/",
      "dest": "/agents/master/handler.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "CONTEXT7_API_KEY": "@context7_api_key",
    "LEETCODE_SESSION": "@leetcode_session"
  },
  "build": {
    "env": {
      "PYTHONPATH": "./shared"
    }
  }
}